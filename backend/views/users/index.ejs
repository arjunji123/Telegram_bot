<div class="row">
    <div class="col-12">
        <div class="card">
            <div class="card-body table-responsive">
                <div class="row page_top_bar">
                    <div class="col-12">
                        <div class="col-6 heading float-left">
                            <h4 class="mt-0 header-title">Users</h4>
                        </div>
                        <div class="col-6 btns float-left text-right">
                            <a class="btn btn-primary" href="/admin/<%= module_slug %>/add">Add New</a>
                        </div>
                    </div>
                </div>

                <table id="responsive-datatable" class="table table-bordered table-bordered dt-responsive nowrap">
                    <thead>
                        <tr>
                            <th>S.No.</th>
                            <th>Name</th>
                            <th>Mobile</th>
                            <th>Email</th>
                            <th>Created</th>
                        </tr>
                    </thead>


                    <tbody>
                        <% let serialNumber=1; %>
                            <% users[0].forEach(function(user) { %>
                                <tr>
                                    <td>
                                        <%= serialNumber++ %>
                                    </td>
                                    <td>
                                        <%= user.name %>
                                    </td>
                                    <td>
                                        <%= user.mobile %>
                                    </td>
                                    <td>
                                        <%= user.email %>
                                    </td>
                                    <td>
                                        <%= user.created_date %>
                                    </td>
                                </tr>
                                <% }); %>


                    </tbody>
                </table>
            </div>
        </div>

    </div>
</div> <!-- end row -->